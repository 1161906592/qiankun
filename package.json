{
  "name": "qiankun",
  "version": "0.0.1",
  "description": "An completed implementation of Micro Frontends",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "start": "npm run start:main & npm run start:react16 & npm run start:vue",
    "build:examples": "cd examples/main && npm i && cd ../react && npm i && cd ../vue && npm i",
    "start:main": "cd examples/main && npm start",
    "start:react16": "cd examples/react && npm start",
    "start:vue": "cd examples/vue && npm start",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "rm -fr esm && tsc",
    "build:cjs": "rm -fr lib && tsc -p tsconfig.cjs.json",
    "prepush": "npm run lint",
    "lint": "tslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuitos/qiankun.git"
  },
  "files": [
    "esm",
    "lib"
  ],
