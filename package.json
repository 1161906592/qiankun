  "bugs": {
    "url": "https://github.com/kuitos/qiankun/issues"
  },
  "homepage": "https://github.com/kuitos/qiankun#readme",
  "dependencies": {
    "import-html-entry": "^0.4.12",
    "lodash": "^4.17.11",
    "single-spa": "^4.3.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.129",
    "@types/node": "^12.0.2",
    "husky": "^2.3.0",
    "tslint": "^5.16.0",
